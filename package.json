    "jest": "^30.0.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~5.5.4"
  },
  "dependencies": {
    "@angular/core": "^18.2.12",
    "@awesome-cordova-plugins/core": "^6.16.0",
    "@awesome-cordova-plugins/http": "^6.16.0",
    "big-integer": "^1.6.52",
    "node-forge": "^1.3.1",
    "rxjs": "^7.8.1",
    "uuid": "^11.1.0"
